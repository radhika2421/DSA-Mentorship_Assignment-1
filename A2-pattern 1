import java.util.Scanner;

public class pattern1 {
    public static void main(String[] args) {
        try (Scanner scn = new Scanner(System.in)) {
            int row = scn.nextInt();
            int col = scn.nextInt();
            for (int i = 0; i < row; i++) {
                for (int j = 0; j < col; j++) {
                    System.out.print("*");
                }
                System.out.println();
            }
        }
    }
}
